<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.izhangqian.checkintool">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:testOnly="false"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.UtilActivity" />
        <activity android:name="com.izhangqian.checkintool.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.izhangqian.checkintool.activity.CheckChooseActivity" />
        <service
            android:name="com.izhangqian.checkintool.service.JobService"
            android:enabled="true"
            android:exported="true" />
        <service
            android:name="com.izhangqian.checkintool.service.JobIntentService"
            android:exported="false" />
        <service
            android:name="com.izhangqian.checkintool.service.MyIntentService"
            android:exported="false" />

        <service
            android:name="com.izhangqian.checkintool.service.MyService"
            android:permission="android.permission.BIND_ACCESSIBILITY_SERVICE">
            <intent-filter>
                <action android:name="android.accessibilityservice.AccessibilityService" />
            </intent-filter>

            <meta-data
                android:name="android.accessibilityservice"
                android:resource="@xml/accessible_service_config_ali" />
        </service>
    </application>

</manifest>